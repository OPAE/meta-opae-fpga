+  * UART related constants
+  ******************************************************************************/
+-#define PLAT_UART0_BASE		(0xFFC02000)
+-#define PLAT_UART1_BASE		(0xFFC02100)
++#define PLAT_UART1_BASE		(0xFFC02000)
++#define PLAT_UART0_BASE		(0xFFC02100)
